to binary representation.\n  let binarioNumberArray: number[] = [];\n  while (allergen_identifier > 1) {\n    binarioNumberArray.unshift(allergen_identifier % 2);\n    allergen_identifier = Math.floor(allergen_identifier / 2);\n  }\n  binarioNumberArray.unshift(allergen_identifier % 2);\n  // Pad the binary array with zeros to make it 8 digits long.\n  while (binarioNumberArray.length < 8) {\n    binarioNumberArray.unshift(0);\n  }\n  // Add allergens to the list based on the binary representation.\n  for (let i = binarioNumberArray.length - 1; i >= 0; --i) {\n    if (binarioNumberArray[i] == 1) allergens.push(signals[i]);\n  }\n  // Return the list of allergens.\n  return allergens;\n}\n\n// console.log(getAllergens(3)); // All allergens.\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"3829e344ec3a95120b7fd13cb99e77d800f6f598","contentHash":"cd6310ddc9d3d5fb93c2641375b4acb80bbb162dc8edabef51c7fae3945e5ca1"},"/home/usuario/ull-esit-inf-dsi-23-24-prct05-objects-classes-interfaces-AlejandroJ22/src/ejercicio_1/BibliographicReference.ts":{"path":"/home/usuario/ull-esit-inf-dsi-23-24-prct05-objects-classes-interfaces-AlejandroJ22/src/ejercicio_1/BibliographicReference.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":60}},"2":{"start":{"line":10,"column":4},"end":{"line":12,"column":5}},"3":{"start":{"line":11,"column":8},"end":{"line":11,"column":21}},"4":{"start":{"line":13,"column":4},"end":{"line":13,"column":16}},"5":{"start":{"line":15,"column":0},"end":{"line":15,"column":30}},"6":{"start":{"line":40,"column":8},"end":{"line":40,"column":27}},"7":{"start":{"line":41,"column":8},"end":{"line":41,"column":31}},"8":{"start":{"line":42,"column":8},"end":{"line":42,"column":33}},"9":{"start":{"line":43,"column":8},"end":{"line":43,"column":31}},"10":{"start":{"line":44,"column":8},"end":{"line":44,"column":43}},"11":{"start":{"line":45,"column":8},"e